<!DOCTYPE html>
{% load staticfiles %}
{% load tags %}
<html lang="pl">
	<head>

        <script>
          (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
          (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
          m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
          })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

          ga('create', 'UA-45651946-1', 'auto');
          ga('send', 'pageview');

        </script>

        <link rel="stylesheet" type="text/css" href="{{ STATIC_URL }}css/style.css" />
        <script type="text/javascript" src="{{ STATIC_URL }}js/jquery-1.11.1.min.js"></script>
        <script type="text/javascript" src="{{ STATIC_URL }}js/jquery-ui.min.js"></script>
        <script type="text/javascript" src="{{ STATIC_URL }}js/slider.js"></script>
        <script type="text/javascript" src="{{ STATIC_URL }}js/productSlider.js"></script>
        <script type="text/javascript" src="{{ STATIC_URL }}js/shop.js"></script>

        <script type="text/javascript" src="{{ STATIC_URL }}js/mustache.js"></script>
        <script type="text/javascript" src="{{ STATIC_URL }}js/handlebars.js"></script>
        
	<script type="text/javascript" src="{{ STATIC_URL }}js/bootstrap-slider.js"></script>
	<link rel="stylesheet" type="text/css" href="{{ STATIC_URL }}css/bootstrap-slider.css" />
	
        <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.1/css/bootstrap.min.css">


        <link rel="stylesheet" type="text/css" href="{{ STATIC_URL }}css/ie.css" />

        <!-- Optional theme -->
        <!--<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.1/css/bootstrap-theme.min.css">-->
        <link rel="stylesheet" href="{{ STATIC_URL }}css/theme2.css" >

        <!-- Latest compiled and minified JavaScript -->
        <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.1/js/bootstrap.min.js"></script>

        {% block scripts %}
        {% endblock %}

    </head>
    <body>
        <div style="padding:0px; border-width:0px" class="alert alert-success" role="alert" id="top-message">

        </div>

            <div id="mask" style="display:none;" class="col-md-10 col-md-offset-1 col-sm-12 col-sx-12"></div>
            <div class="col-md-10 col-md-offset-1 page-container col-sm-12 col-sx-12">
                {% include 'header.djhtml' %}
                <form id="variables">
                    <input type="hidden" id="sessionId" value="{{ request.session.session_id }}"></input>
                </form>
                <div class="container-fluid">
                        {% block body %}
                        {% endblock %}
                </div>

                <div style="clear:both"></div>
                {% display_template name='footer' %}
            </div>
    </body>
    {% block addScripts %}
    {% endblock %}
</html>
